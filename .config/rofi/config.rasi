configuration {
  sidebar-mode:  true;
  hide-scrollbar: true;
  kb-cancel: "Escape,Alt+q";
  kb-mode-next: "Alt+l,Alt+n";
  kb-mode-previous: "Alt+h,Alt+N";
  kb-row-up: "Alt+k,Up";
  kb-row-down: "Alt+j,Down";
  modi: "combi,drun,run,window,ssh";
  combi-modi: "window,drun,run";
  show: "combi";
}
@import "/usr/share/rofi/themes/gruvbox-dark-soft.rasi"
